{
  "name": "todots",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=development jest",
    "test:e2e": "cross-env NODE_ENV=test jest --testPathPattern=src/e2e",
    "build": "tsc --build",
    "start": "node ./dist/index",
    "dev": "nodemon ./src/index.ts",
    "db_setup": "node ./src/db_setup.mjs",
    "db:migration:generate": "sequelize migration:generate --config ./src/db/config.sequelize.js --migrations-path ./src/db/migrations",
    "db:migrate": "sequelize db:migrate --config ./src/db/config.sequelize.js --migrations-path ./src/db/migrations",
    "db:migrate:undo": "sequelize db:migrate:undo --config ./src/db/config.sequelize.js --migrations-path ./src/db/migrations"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.12.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
